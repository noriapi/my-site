    "test:playwright": "playwright test",
    "docker:test": "docker compose up --abort-on-container-exit --build"
  },
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "@ark-ui/solid": "^5.14.0",
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.5.4",
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/sitemap": "^3.4.1",
    "@astrojs/solid-js": "^5.1.0",
    "@solid-primitives/media": "^2.3.1",
    "@solid-primitives/storage": "^4.3.2",
    "astro": "^5.9.2",
    "dayjs": "^1.11.13",
    "is-absolute-url": "^4.0.1",
    "solid-js": "^1.9.7",
    "typescript": "^5.8.3"
