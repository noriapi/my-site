{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "my-site",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2025-06-10",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist",
  },
  "observability": {
    "enabled": true,
  },
  "vars": { "PNPM_VERSION": "10.11.1" },
  "routes": [
    {
      "pattern": "noriapi.com",
      "custom_domain": true,
    },
  ],
}
