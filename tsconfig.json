{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "types": [
      "./worker-configuration.d.ts",
      "node",
      "unplugin-icons/types/solid"
    ],
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",

    "noFallthroughCasesInSwitch": true
  }
}
